-
  const contactsCentersRegion = [
    {
      title: 'Дайв-центры в Подмосковье',
      list: [
        {
          classItem: 'centers__location-item--active',
          classBtn: 'centers__location-btn--active',
          name: 'Дайв-центр «Cпас-Каменка»',
          address: 'Подмосковье, Спас-Каменка, Дмитровский район',
          src: 'https://yandex.ru/map-widget/v1/?ll=37.456509%2C56.148097&mode=search&ol=geo&ouri=ymapsbm1%3A%2F%2Fgeo%3Fdata%3DCgg1MzA2MDIzNBKPAdCg0L7RgdGB0LjRjywg0JzQvtGB0LrQvtCy0YHQutCw0Y8g0L7QsdC70LDRgdGC0YwsINCU0LzQuNGC0YDQvtCy0YHQutC40Lkg0LPQvtGA0L7QtNGB0LrQvtC5INC-0LrRgNGD0LMsINC00LXRgNC10LLQvdGPINCh0L_QsNGBLdCa0LDQvNC10L3QutCwIgoN59UVQhVvmmBC&z=14.3',
        },
      ],
    },
  ];
  const contactsRegion = [
    {
      route: [
        {
          title: 'от ст. м. "Алтуфьево": ',
          text: 'Первый вагон из центра, направо, затем налево. Маршрутка №270 до поворота на Спас-Каменку.',
        },
        {
          title: 'С собой ',
          text: 'нужно взять лог-бук и сертификаты.',
        },
      ],
      time: 'По выходным в летний и зимний сезоны, заезд начинается вечером пятницы.',
      phone: '+7 (916) 925-77-99',
      class: 'centers__about-text--active',
    },
  ]

.centers.contacts-offices
  .centers__block.contacts-offices__block
    for item in contactsCentersRegion
      +centersListBlock('contacts-offices', item)
      .centers__map-block.contacts-offices__map-block
        .centers__map
      .centers__about.contacts-offices__about
        for item, index in contactsRegion
          .centers__about-text.contacts-offices__about-content(class=item.class data-loc-text=`location-${index + 1}`)
            for route in item.route
              .contacts-offices__route
                span.contacts-offices__about-title= route.title
                span.contacts-offices__about-text= route.text
            .contacts-offices__time
              span.contacts-offices__about-title Часы работы:&nbsp;
              span.contacts-offices__about-text= item.time
            .contacts-offices__phone
              span.contacts-offices__about-title Тел.:&nbsp;
              span.contacts-offices__about-text= item.phone
